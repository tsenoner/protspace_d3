{
  "name": "protspace_d3",
  "private": true,
  "description": "ProtSpace interactive protein visualization components",
  "packageManager": "pnpm@10.11.0",
  "scripts": {
    "build": "turbo build",
    "dev": "turbo run dev",
    "dev:app": "turbo run dev --filter=protspace-app",
    "dev:core": "turbo run --filter @protspace/example-scatterplot dev",
    "test": "turbo test",
    "lint": "turbo lint",
    "type-check": "turbo type-check",
    "clean": "turbo clean",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "turbo build && changeset publish"
  },
  "keywords": [],
  "author": "",
  "devDependencies": {
    "@changesets/cli": "^2.29.4",
    "@eslint/eslintrc": "^3.3.1",
    "turbo": "^2.5.3"
  }
}
